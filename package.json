{
  "name": "di-txma-audit",
  "version": "1.0.0",
  "description": "TxMA Audit",
  "repository": {
    "type": "git",
    "repository": "https://github.com/alphagov/di-txma-audit"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@aws-crypto/encrypt-node": "3.1.0",
    "@aws-crypto/kms-keyring-node": "3.1.0",
    "@aws-crypto/material-management": "3.1.0",
    "@aws-sdk/client-s3": "3.257.0"
  },
  "devDependencies": {
    "@jest/types": "28.1.3",
    "@types/aws-lambda": "8.10.102",
    "@types/jest": "28.1.7",
    "@types/jest-when": "3.5.2",
    "@types/node": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.51.0",
    "aws-sdk-client-mock": "2.0.1",
    "aws-sdk-client-mock-jest": "2.0.1",
    "esbuild": "0.17.7",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "27.2.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-when": "3.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.3",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yaml-cfn": "0.3.2"
  },
  "lint-staged": {
    "*": "prettier --write .",
    "*.{js,ts}": "eslint --fix ."
  }
}

{
  "name": "txma-audit",
  "version": "1.0.0",
  "description": "TxMA Audit",
  "repository": {
    "type": "git",
    "repository": "https://github.com/govuk-one-login/txma-audit"
  },
  "author": "GDS",
  "license": "MIT",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "build": "tsc --noEmit && ts-node ./esbuild.config.ts",
    "deploy:dev": "npm run build && sam deploy --resolve-s3",
    "lint": "prettier . --check || exit 1 ; eslint . --max-warnings=0",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:integration": "jest -c ./tests/integrationTests/jest.config.integration.ts",
    "test:e2e": "jest -c ./tests/e2eTests/jest.config.e2e.ts",
    "tsFile": "ts-node"
  },
  "dependencies": {
    "@aws-crypto/client-node": "4.2.1",
    "@aws-crypto/encrypt-node": "4.2.1",
    "@aws-crypto/material-management": "4.2.1",
    "@aws-lambda-powertools/logger": "2.27.0",
    "@aws-sdk/client-firehose": "3.901.0",
    "@aws-sdk/client-s3": "3.901.0",
    "@aws-sdk/client-s3-control": "3.901.0",
    "aws-xray-sdk-core": "3.10.3"
  },
  "resolutions": {
    "micromatch": "4.0.8"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.902.0",
    "@aws-sdk/client-cloudwatch-logs": "3.901.0",
    "@aws-sdk/client-lambda": "3.901.0",
    "@aws-sdk/client-secrets-manager": "3.901.0",
    "@aws-sdk/client-ssm": "3.901.0",
    "@aws-sdk/client-sso-oidc": "^3.901.0",
    "@aws-sdk/client-sts": "3.901.0",
    "@aws-sdk/util-endpoints": "3.901.0",
    "@eslint/js": "^9.37.0",
    "@jest/types": "30.2.0",
    "@types/aws-lambda": "8.10.155",
    "@types/jest": "30.0.0",
    "@types/jest-when": "3.5.5",
    "@types/node": "24.7.0",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-jest": "4.1.0",
    "commander": "14.0.1",
    "esbuild": "0.25.10",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "globals": "^16.4.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "jest-when": "3.7.0",
    "lint-staged": "16.2.3",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "yaml-cfn": "0.3.2"
  },
  "lint-staged": {
    "*": "prettier --write .",
    "*.{js,ts}": "eslint --fix ."
  }
}

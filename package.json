    "@aws-sdk/util-endpoints": "3.743.0",
    "@eslint/js": "^9.23.0",
    "@jest/types": "29.6.3",
    "@types/aws-lambda": "8.10.148",
    "@types/jest": "29.5.14",
    "@types/jest-when": "3.5.5",
    "@types/node": "22.13.13",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-jest": "4.1.0",
    "commander": "14.0.0",
    "esbuild": "0.25.1",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-jest": "28.11.0",
    "globals": "^16.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-when": "3.7.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.27.0",
    "yaml-cfn": "0.3.2"
  },
  "lint-staged": {
    "*": "prettier --write .",
    "*.{js,ts}": "eslint --fix ."
  }
